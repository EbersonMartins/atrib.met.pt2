import java.sql.SQLOutput;
import java.util.Scanner;

public class Principal {
    public static void main(String[] args) {

     Scanner sc = new Scanner(System.in);
     Conta c = new Conta();

        System.out.println(" Digite seu CPF : ");
        c.cpf = sc.nextLine();
        System.out.println(" Digite o numero da conta : ");
        c.numBanco = sc.nextLine();
        System.out.println(" Quanto você quer sacar ? ");
        c.saque = sc.nextDouble();
        System.out.println(" Quanto você vai depositar ? ");
        c.deposito = sc.nextDouble();
        System.out.println(" Você é um cliente especial ? ");
        c.tipoCliente = sc.nextLine();

        System.out.println(" O seu saldo ficou de  R$" + c.saldo);
        
        
        
        
        



sc.close();


    }
}
